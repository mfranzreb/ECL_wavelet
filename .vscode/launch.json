    {
      "name": "Debug kernel tuner CUDA",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${workspaceFolder}/build/kernel_tuning/ecl_tune_kernels",
      "args": [". 0"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build kernel tuner"
    },
    {
      "name": "Debug kernel tuner",
      "type": "cppdbg",
