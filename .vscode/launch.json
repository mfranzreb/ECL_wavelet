          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build bitarray benchmarks"
    },
    {
      "name": "Debug plot",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/benchmark/plot_hist_comp.py",
      "args": ["benchmark/results/bm_hist_computation.json", "."],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug tune plot",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/kernel_tuning/plot_kernel_tunes.py",
      "args": ["kernel_tuning/FLK.csv", "."],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug tree tests CUDA",
