  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/main",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Debug bitarray tests CUDA",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${workspaceFolder}/build/tests/ecl_BA_RS_tests",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build bitarray tests"
    },
    {
      "name": "Debug bitarray tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/tests/ecl_BA_RS_tests",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build bitarray tests"
    },
    {
      "name": "Debug bitarray benchmarks CUDA",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${workspaceFolder}/build/benchmark/ecl_RS_benchmarks",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build bitarray benchmarks"
    },
    {
      "name": "Debug bitarray benchmarks",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/benchmark/ecl_RS_benchmarks",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
